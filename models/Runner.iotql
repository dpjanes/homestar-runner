--
--  compile to JSON with 'iotql-model Runner.iotql'
--
CREATE MODEL Runner WITH
    schema:name = "Runner",
    schema:description = "Runner"
ATTRIBUTE on WITH
    schema:name = "on",
    iot:purpose = iot-purpose:on,
    iot:type = iot:type.boolean,
    iot:write = true,
    iot:read = true,
    iot:actuator = true,
    iot:sensor = true
;
